<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0dp" />

            <gradient
                android:centerColor="@color/color_webview_progressbar_background"
                android:endColor="@color/color_webview_progressbar_background"
                android:startColor="@color/color_webview_progressbar_background" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="0dp" />

                <gradient
                    android:centerColor="@color/color_webview_progressbar_background"
                    android:endColor="@color/color_webview_progressbar_background"
                    android:startColor="@color/color_webview_progressbar_background" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0dp" />

                <gradient
                    android:centerColor="@color/color_webview_progressbar_progress"
                    android:endColor="@color/color_webview_progressbar_progress"
                    android:startColor="@color/color_webview_progressbar_progress" />
            </shape>
        </clip>
    </item>

</layer-list>