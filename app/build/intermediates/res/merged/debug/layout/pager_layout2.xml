<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >

    <View
        android:layout_width="@dimen/main_icon_margin_left"
        android:layout_height="fill_parent"
        android:background="@null" />

    <!-- 1-All function -->

    <FrameLayout
        android:id="@+id/main_all_function"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/btn_fuction_item_bg"
        android:foreground="@drawable/butten_mask"
        android:gravity="center"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_home_all_function" />

            <TextView
                android:id="@+id/bluetooth_text"
                style="@style/text_session_item_f4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/homepage_text_margin_icon"
                android:text="@string/main_all_function" />
        </LinearLayout>
    </FrameLayout>
<!-- 
    <View
        android:id="@+id/line1"
        android:layout_width="2dp"
        android:layout_height="fill_parent"
        android:background="@null" />
 -->
    <!-- 2-Settings -->

    <FrameLayout
        android:id="@+id/setting"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/btn_fuction_item_bg"
        android:foreground="@drawable/butten_mask"
        android:gravity="center"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/setting_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_home_setting" />

            <TextView
                android:id="@+id/setting_text"
                style="@style/text_session_item_f4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/homepage_text_margin_icon"
                android:text="@string/common_control" />
        </LinearLayout>
    </FrameLayout>
<!--
    <View
        android:id="@+id/line2"
        android:layout_width="2dp"
        android:layout_height="fill_parent"
        android:background="@null"
        android:visibility="invisible" />
  -->
    <!-- 3-stock invisible -->

    <FrameLayout
        android:id="@+id/stock"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/btn_fuction_item_bg"
        android:foreground="@drawable/butten_mask"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="invisible" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/music_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_home_setting" />

            <TextView
                android:id="@+id/music_text"
                style="@style/text_session_item_f4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/homepage_text_margin_icon"
                android:text="@string/stock_info" />
        </LinearLayout>
    </FrameLayout>
<!--
    <View
        android:id="@+id/line3"
        android:layout_width="2dp"
        android:layout_height="fill_parent"
        android:background="@null"
        android:visibility="invisible" />
  -->
    <!-- 4-null invisible -->

    <FrameLayout
        android:id="@+id/spare4"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/btn_fuction_item_bg"
        android:foreground="@drawable/butten_mask"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="invisible" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/poi_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_home_setting" />

            <TextView
                android:id="@+id/poi_text"
                style="@style/text_session_item_f4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/homepage_text_margin_icon"
                android:text="@string/map_navigation" />
        </LinearLayout>
    </FrameLayout>
<!-- 
    <View
        android:id="@+id/line4"
